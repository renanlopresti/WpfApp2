<Window x:Class="WpfApp2.Entidades.AddTentativa"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2.Entidades"
        mc:Ignorable="d"
        Title="AddTentativa" Height="450" Width="326">
    <Grid>
        <StackPanel Margin="49,114,49,114">
            <TextBlock TextWrapping="Wrap" Text="Adicionar Manobra" RenderTransformOrigin="0.355,1.036" HorizontalAlignment="Center" Margin="0,0,0,10" FontSize="16" FontWeight="Bold"/>
            <Label Content="Pontos:"/>
            <TextBox TextWrapping="Wrap" Text="{Binding Valor}" Width="120" Margin="0,0,0,10"/>
            <Label Content="Manobra:"/>
            <ComboBox SelectedValue="{Binding Manobra}" x:Name="ComboBox1" Width="120" Margin="0,0,0,25"/>
            <Button Content="Adicionar Manobra" Click="bnt_salvar"/>
        </StackPanel>

    </Grid>
</Window>
